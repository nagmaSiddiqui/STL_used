#include<iostream>
#include<list>

using namespace std;
int main(){
    //list creation
    list<int>l;
    
    //copy list in new list
    list<int>n(5,100);
    cout<<"print n:"<<endl;
    for (int i:n){
        cout<<i<<" ";
    }
    
    //include elements in list
    l.push_back(1);
    l.push_front(2);
    //for print list
    for (int i:l){
        cout<<i<<" ";
    }
    cout<<endl;
    l.erase(l.begin());
    cout<<"after erase"<<endl;
    for (int i:l){
        cout<<i<<" ";
    }
    cout<<endl;
    cout<<"size of list:"<<l.size()<<endl;
    
  
    
}
