#include<iostream>
#include<queue>

using namespace std;
int main(){
  // create queue 
  queue<string>q;
  // now this is empty so insert element in it
  q.push("what");
  q.push("is");
  q.push("this");
  
  cout<<"Top element:"<<q.front()<<endl;
  q.pop();
  cout<<"Top element:"<<q.front()<<endl;
  cout<<"size after pop:"<<q.size()<<endl;
  
  
    
}
    
